{
  "name": "keleya-pregnancy-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "detox test --configuration ios.sim.debug",
    "test:build": "detox build --configuration ios.sim.debug",
    "type-check": "tsc",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "check-updates": "npm-check --skip-unused",
    "reset:android": "rm -rf android/build && rm -rf android/app/build",
    "reset:ios": "cd ios && rm Podfile.lock && pod deintegrate && rm -rf Pods && pod install --repo-update && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf /Users/${USER}/Library/Developer/Xcode/DerivedData && xcodebuild clean",
    "reset:watchman": "rm -rf $TMPDIR/metro-cache* && rm -rf /tmp/metro-* && watchman watch-del-all && watchman shutdown-server",
    "reset": "yarn cache clean && rm -rf node_modules && rm yarn.lock && yarn install --network-concurrency 1 && watchman watch-del-all && npx browserslist@latest --update-db",
    "reset-all:android": "yarn reset && yarn reset:android && yarn reset:watchman",
    "reset-all:ios": "yarn reset && yarn reset:ios && yarn reset:watchman"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^7.5.0",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-native-picker/picker": "^2.5.0",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "dayjs": "^1.11.9",
    "i18next": "^23.2.3",
    "react": "18.2.0",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^13.0.1",
    "react-native": "0.72.1",
    "react-native-bouncy-checkbox": "^3.0.7",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-localize": "^3.0.2",
    "react-native-mmkv": "^2.10.0",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-reanimated": "^3.3.0",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^3.22.0",
    "react-native-size-matters": "^0.4.0",
    "react-redux": "^8.1.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@react-navigation/devtools": "^6.0.19",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.4.0",
    "@types/metro-config": "^0.76.3",
    "@types/node": "^18.14.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-root-import": "^6.6.0",
    "detox": "^20.11.4",
    "dotenv": "^16.3.1",
    "eslint": "^8.19.0",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "^29",
    "metro-react-native-babel-preset": "0.76.5",
    "prettier": "^2.4.1",
    "react-native-flipper": "^0.202.0",
    "react-test-renderer": "18.2.0",
    "redux-flipper": "^2.0.2",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
